{
  "name": "metadatering",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "typings": "index",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  },
  "bin": {
    "generic-hypermedia-api-client": "./bin/generic-hypermedia-api-client.js"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build"
  },
  "author": "Dwight Van Lancker",
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts",
    "lib/*.d.ts",
    "lib/*.js"
  ],
  "dependencies": {
    "@rdfjs/data-model": "^1.1.0",
    "content-type": "^1.0.4",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "minimist": "^1.2.0",
    "parse-link-header": "^1.0.1",
    "rdf": "^4.0.1",
    "rdf-ext": "^1.1.0",
    "rdf-formats-common": "^1.4.0",
    "rdf-js": "^1.0.1",
    "rdf-string": "^1.2.0",
    "stream": "0.0.2",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.5",
    "arrayify-stream": "^1.0.0",
    "fetch-mock": "^6.5.2",
    "jest": "^23.5.0",
    "node-fetch": "^2.2.0",
    "string-to-stream": "^1.1.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.1"
  }
}
