{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "arrow-parens": false,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "ordered-imports": false,
        "align": false,
        "object-literal-sort-keys": false,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-empty": false,
        "member-access": false
    },
    "linterOptions": {
        "exclude": [
            "dist/**/*",
            "**/node_modules/**/*"
        ]
    }
}
